{
  "name": "finstagram",
  "version": "1.0.0",
  "scripts": {
    "start": "start cmd /c \"cd api && npm start\" && npm run local",
    "local": "vue-cli-service serve --mode dev",
    "build": "vue-cli-service build --mode live",
    "live": "node ./server",
    "lint": "vue-cli-service lint"
  },
  "description": "The Instagram that the people deserve. A picture sharing platform with honest reactions for down to earth users who won't shy away from a little constructive criticism. Users can post pictures to which others can react to or comment on.",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "bootstrap-vue": "^2.3.0",
    "express": "^4.17.1",
    "v-viewer": "^1.5.1",
    "vue": "2.5.17",
    "vue-router": "^3.1.5",
    "vue-sweetalert2": "^3.0.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.4",
    "@vue/cli-plugin-eslint": "3.0.4",
    "@vue/cli-service": "3.0.4",
    "eslint": "5.6.0",
    "eslint-plugin-vue": "4.7.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
